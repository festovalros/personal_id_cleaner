<?xml version="1.0"?>
<openerp>
<data>

    <record model="ir.ui.view" id="view_CleanPersonalID_form">
        <field name="name">CleanPersonalID.form</field>
        <field name="model">CleanPersonalID</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="CleanPersonalID">
            <group col="4" colspan="2">
                <field name="dirty" select="1"/>
                <field name="clean" select="2"/>

            </group>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_CleanPersonalID_tree">
        <field name="name">CleanPersonalID.tree</field>
        <field name="model">CleanPersonalID</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="CleanPersonalID">
                <field name="dirty"/>
                <field name="clean"/>

            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_CleanPersonalID">
        <field name="name">PersonalID</field>
        <field name="res_model">CleanPersonalID</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem name="PersonalID" id="menu_CleanPersonalID" action="action_CleanPersonalID"/>

        
</data>
</openerp>